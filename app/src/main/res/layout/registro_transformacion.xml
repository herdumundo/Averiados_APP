        android:text="CANTIDAD"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/txt_cantidad_transformacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
