        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="0dp"
        android:background="@android:color/holo_blue_dark"
        android:drawableEnd="@drawable/ic_action_spinner"
        android:fontFamily="monospace"
        android:text="SELECCIONE DEPOSITO"
        android:textColor="#000000"
        android:textColorHighlight="#AC6969"
        android:textColorLink="#935F5F"
        android:textCursorDrawable="@android:color/holo_green_dark"
        android:textSize="19sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,RtlCompat" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="227dp"
        android:layout_marginEnd="0dp"
        android:background="#070707"
        android:backgroundTint="@android:color/background_dark"
        android:text="DETALLE"

        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic" />

    <TextView
        android:id="@+id/txt_a_tipo_huevo"
