<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.averiados.menu_informe">

    <Button
        android:id="@+id/btn_informe_cambio"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="16dp"
        android:drawableLeft="@drawable/ic_action_info_cambio"
        android:onClick="ir_informe_cambios"
        android:text="INFORME REGISTRO CAMBIOS"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_informe_desmontaje"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_informe_cambio"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:drawableLeft="@drawable/ic_action_info_desmontaje"
        android:onClick="ir_informe_desmontajes"
        android:text="INFORME REGISTRO DESMONTAJES"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="249dp" />

    <Button
        android:id="@+id/btn_informe_devoluciones"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_informe_desmontaje"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:drawableLeft="@drawable/ic_action_info_devoluciones"
        android:onClick="ir_informe_devoluciones"
        android:text="INFORME REGISTRO DEVOLUCIONES"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"


        tools:layout_editor_absoluteY="163dp" />

    <Button
        android:id="@+id/btn_informe_transferencia"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_informe_devoluciones"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="17dp"
        android:drawableLeft="@drawable/ic_action_info_transferencia"
        android:onClick="ir_informe_transferencias"
        android:text="INFORME REGISTRO TRANSFERENCIAS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="163dp" />

    <Button
        android:id="@+id/btn_informe_stock"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_informe_transferencia"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="18dp"
        android:drawableLeft="@drawable/ic_action_info_transferencia"
        android:onClick="ir_informe_stock"
        android:text="INFORME STOCK DE HUEVOS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="163dp" />

    <Button
        android:id="@+id/btn_resumen"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_resumen2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="16dp"
        android:drawableLeft="@drawable/ic_action_info_transferencia"
        android:onClick="ir_resumen_desmontaje"
        android:text="RESUMEN DESMONTAJE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="163dp" />

    <Button
        android:id="@+id/btn_resumen2"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_informe_stock"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="16dp"
        android:drawableLeft="@drawable/ic_action_info_transferencia"
        android:onClick="ir_informe_stock_averiados"
        android:text="INFORME STOCK DE HUEVOS DESECHOS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="163dp" />

</RelativeLayout>