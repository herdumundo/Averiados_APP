            android:layout_width="match_parent"
            android:layout_height="65dp"

            android:hint="INGRESE CANTIDAD"
            android:inputType="number"
            android:maxLines="1"
            android:singleLine="true"
            android:textColorHighlight="#000000"
            android:textColorLink="#050505" />

    </android.support.design.widget.TextInputLayout>




    <TextView
        android:id="@+id/txt_cant_disponible_transferencia"
        android:layout_width="151dp"
        android:layout_height="37dp"
        android:layout_below="@+id/txt_cant_disponible3"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="9dp"
        android:ems="10"
        android:hint="0"
        android:text="0"
        android:textColor="#FF0000"
        android:textColorHint="#232121"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="301dp" />

    <TextView
        android:id="@+id/txt_cant_disponible3"
        android:layout_width="157dp"
        android:layout_height="43dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
